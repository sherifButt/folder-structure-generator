{
  "source": {
    "include": [
      "./"
    ],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(./node_modules/|./public|./docs|./alerts)"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": [
    "plugins/markdown",
    "jsdoc-mermaid",
    "docs/super-docs/typescript",
    "docs/super-docs/category",
    "docs/super-docs/component"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    
    "search": true,
    "default": {
      "staticFiles": {
        "include": [
          "./docs/statics"
        ]
      }
    },
    "favicon": "./favicon.png",
    "super-docs": {
     "name": "Super Docs",
      "logo": "images/logo.png",
      "title": "Super docs",
      "css": "styles/style.css",
      "changelog":"./CHANGELOG.md",
      "hideGenerator": true,
      "favicon": "images/favicon.ico",
      "faviconType": "image/x-icon",
      "swagger": "./config/swagger.js",
      
